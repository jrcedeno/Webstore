<% flash.each do |key, value| %>
	<% result = flash_class(key) %>
		<div class="<%= flash_class(key) %>  role="alert">
		    <buttonclass="close" data-dismiss="alert" type="button">
		      <span aria-hidden="true"> &times;</span>
		      <span class="sr-only"> Close</span>
		     </button>
		     <%= value %>
		  </div>
<% end %>